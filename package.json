{
  "name": "timereminder",
  "version": "0.0.1",
  "description": "Manage your time locally",
  "main" : "./dist/electron.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "webpack --config ./webpack.config.js",
    "start": "npm run build && electron ./dist/electron.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "nodemon": "nodemon --watch * --exec \"electron ./dist/electron.js\""
  },
  "keywords": [],
  "author": "Simon Palmberg",
  "license": "ISC",
  "build": {
    "appId": "com.example.timereminder",
    "productName": "timeReminder",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "./dist",
      "output": "./electron-build"
    },
    "mac": {
      "category": "public.private.calendar",
      "icon": "dist/icon.icns"
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-loader": "^9.4.2",
    "ttf-loader": "^1.0.2",
    "typescript": "^3.7.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "css-loader": "^6.7.3",
    "electron-store": "^5.1.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "react-autocomplete": "^1.8.1",
    "react-big-calendar": "^0.23.0",
    "react-datepicker": "^2.10.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1"
  }
}
